<Window x:Class="AsyncExampleWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AsyncExampleWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="resultsTextBox" HorizontalAlignment="Left" Height="250" Margin="192,136,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="500" FontFamily="Lucida Console" RenderTransformOrigin="0.228,0.314"/>
        <Button x:Name="startButton" Click="StartButton_Click"
                Content="Start" HorizontalAlignment="Left" Margin="134,42,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.722,-0.088"/>

    </Grid>
</Window>
